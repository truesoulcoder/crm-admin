{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/"
    }
  ],
  "functions": {
    "src/app/api/document-templates/**/*.ts": {
      "maxDuration": 30,
      "memory": 1024
    },
    "pages/api/**/*.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
