{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/",
      "dest": "/"
    }
  ],
    "functions": {
      "pages/api/eli5-engine/**/*.ts": {
        "includeFiles": "node_modules/@sparticuz/chromium/lib/**"
    },
    "pages/api/**/*.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
