{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "functions": {
    "src/app/api/leads/upload/route.ts": {
      "maxDuration": 300,
      "memory": 1024,
      "maxBodyLength": 52428800 
    }
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "regions": ["iad1"]
}
